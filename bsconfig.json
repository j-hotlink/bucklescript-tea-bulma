{
    "name": "bucklescript-tea-bulma",
    "version": "0.0.1",
    "package-specs": {
        "module": "es6",  // module 'es6' format is required by the rollup utility, so replaces 'commonjs'
        "in-source": true
    },
    "suffix": ".bs.js",
    "bs-dependencies": [
        "bucklescript-tea"
    ],
    "sources": [
        "src"
    ],
    "warnings": {
        "error" : "+101"
    },
    "generate-merlin" : false,
    "refmt": 3,
    "bsc-flags": [
        "-bs-super-errors"
    ]
}
